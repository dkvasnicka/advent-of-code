-g '*.lisp' -- sh -c 'echo "\033[0;32m-------------------------------- Running {} ---\033[0m" \
    && rainbow --red="(Unhandled .+|\d+:)" --green="\(.+\)" --yellow="\".+\"" --bold="Backtrace .+" --magenta=".+WARNING.+" \
    ros -s parachute -s rove -s cl21 -s iterate -s serapeum -s "f-underscore" -s for -s alexandria -s series {} < `basename {} .lisp`.txt'
